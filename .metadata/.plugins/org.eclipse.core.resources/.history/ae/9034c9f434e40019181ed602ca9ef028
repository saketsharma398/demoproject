package com.test.saket.controllers;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.PostMapping;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

import Service.SaketService;


@RestController
@RequestMapping("/saket")
public class SaketController {

	@Autowired
	SaketService saketController;
	
	
	@GetMapping("/getmapping")
	public String testMethod() {
		return saketController.testMethod();
	}
	@PostMapping("/postmapping")
	public String testMethod1() {
		return "Saket's Post Mapping propgram";
	}
}
